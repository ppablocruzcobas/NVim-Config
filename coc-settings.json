{
  "suggest.echodocSupport": true,
  "suggest.completionItemKindLabels": {
    "text": "t",
    "method": "m",
    "function": ""
  },
  "diagnostic.errorSign": " ",
  "diagnostic.warningSign": " ",
  "diagnostic.infoSign": "",
  "diagnostic.hintSign": " ",
  "diagnostic.virtualText": false,
  "codeLens.enable": true,
  "list.indicator": ">",
  "list.selectedSignText": " ",
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "markdown",
    "javascript",
    "html",
    "python",
    "java",
    "json",
    "typescript"
  ],
  "java.home": "/usr/lib/jvm/java-11-openjdk",
  "python.pythonPath": "/usr/bin/python",
  "python.linting.enabled": true,
  "python.linting.pylintEnabled": true,
  "python.linting.pylintPath": "~/.local/bin/pylint",
  "python.sortImports.path": "~/.local/bin/isort",
  "python.jediPath": "/lib/python3.9/site-packages",
  "coc.source.emoji.filetypes": [
    "markdown"
  ],
  "languageserver": {
    "bash": {
      "command": "bash-language-server",
      "args": [
        "start"
      ],
      "filetypes": [
        "sh"
      ],
      "ignoredRootPaths": [
        "~"
      ]
    }
  },
  "r.lsp.path": "/usr/bin/R",
  "tsserver.npm": "/usr/bin/npm",
  "html.autoClosingTags": true,
  "rest-client.defaultHeaders": {
    "Content-Type": "application/json"
  },
  "rest-client.showHeaders": false,
  "json.enable": true,
  "json.format.enable": true
}
